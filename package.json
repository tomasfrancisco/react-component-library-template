{
  "name": "react-component-library-template",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "engines": {
    "node": "v16.14.0",
    "yarn": "This is an NPM repository. Please use NPM instead."
  },
  "scripts": {
    "test": "jest --config .jest/jest.config.ts",
    "test:e2e": "npm run storybook:build && concurrently \"serve -s storybook-build\" \"jest --config .jest/jest.e2e.config.ts\"",
    "test:watch": "npm run test -- --watchAll",
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.config.dev.ts",
    "build:prod": "webpack --config config/webpack.config.prod.ts",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o ./storybook-build",
    "typecheck": "tsc",
    "loki": "loki --chromeFlags=\"--headless --no-sandbox\"",
    "loki:update": "npm run loki -- update",
    "loki:test": "npm run loki -- test",
    "loki:test:ci": "npm run storybook:build && npm run loki -- --requireReference --reactUri file:./storybook-build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "6.4.0",
    "@storybook/addon-essentials": "6.4.0",
    "@storybook/addon-links": "6.4.0",
    "@storybook/addon-storyshots-puppeteer": "^6.4.19",
    "@storybook/react": "6.4.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.4.0",
    "@types/jest-environment-puppeteer": "^5.0.0",
    "@types/node": "^17.0.16",
    "@types/puppeteer": "^5.4.5",
    "@types/react": "^17.0.39",
    "@types/styled-components": "^5.1.22",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.3",
    "babel-jest": "^27.5.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^2.0.2",
    "concurrently": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.0",
    "jest-puppeteer": "^6.1.0",
    "jest-styled-components": "^7.0.8",
    "loki": "^0.28.1",
    "puppeteer": "^3.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "serve": "^13.0.2",
    "styled-components": "^5.3.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "typescript-plugin-styled-components": "^2.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
}
